{
  "parser": "@typescript-eslint/parser",
  "plugins": ["react", "@typescript-eslint"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "universe"],
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "ignorePatterns": [
    "build/**",
    "**/*.js",
    ".vscode/.react/",
    ".DS_Store",
    "node_modules/",
    ".expo/",
    "dist/",
    "npm-debug.*",
    "*.jks",
    "*.p8",
    "*.p12",
    "*.key",
    "*.mobileprovision",
    "*.orig.*"
  ]
}
